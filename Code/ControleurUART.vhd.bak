USE ieee.std_logic_1164.ALL;

ENTITY ControleurUART IS
	port (
		i_resetBar				: IN	STD_LOGIC;
			i_clock					: IN	STD_LOGIC;
			i_RxD						: IN	STD_LOGIC;
			i_EE						: IN  STD_LOGIC;
			i_ED						: IN  STD_LOGIC;
			i_RRDP					: IN  STD_LOGIC;
			i_BClkx8					: IN  STD_LOGIC;
			o_EE						: IN  STD_LOGIC;
			o_ED						: IN  STD_LOGIC;
			o_RRDP					: IN  STD_LOGIC;
			o_Value					: OUT	STD_LOGIC_VECTOR(7 downto 0)
		);
END ControleurUART;